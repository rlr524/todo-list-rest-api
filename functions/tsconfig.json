{
	"compilerOptions": {
		"module": "NodeNext",
		"esModuleInterop": true,
		"moduleResolution": "nodenext",
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "es2022",
		"lib": ["ES2022"]
	},
	"compileOnSave": true,
	"include": ["src"]
}
